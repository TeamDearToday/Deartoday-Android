<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DearToday" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_b2d1ff</item>
        <item name="colorPrimaryVariant">@color/blue_78a6ff</item>
        <item name="colorOnPrimary">@color/white_ffffff</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow_fee723</item>
        <item name="colorSecondaryVariant">@color/yellow_fff280</item>
        <item name="colorOnSecondary">@color/black_000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
    </style>
</resources>